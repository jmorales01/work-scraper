{
    "builds": [
        {
        "src": "./index.js",
        "use": "@vercel/node"
        }
    ],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "/"
        }
    ],
    "crons": [
        {
        "cron": "*/5 * * * *",
        "dest": "./src/scraper/utpjobs/utp-jobs.js"
        }
    ]
}